{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "markdownFormatter.enable": true,
  "markdownFormatter.fullWidthTurnHalfWidth": "auto",
  "markdownFormatter.codeAreaToBlock": "",
  "markdownFormatter.formatCodes": true,
  "markdownFormatter.formatULSymbol": false,
  "markdownFormatter.formatOpt": {
    "indent_size": 2,
    "e4x": true,
    "brace_style": "collapse,preserve-inline",
    "indent_with_tabs": false,
    "space_after_anon_function": false,
    "space_after_named_function": false
  },
  "[markdown]": {
    "editor.formatOnSave": false,
    "editor.renderWhitespace": "all",
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
    },
    "editor.snippetSuggestions": "top",
    "editor.tabCompletion": "on",
    "editor.acceptSuggestionOnEnter": "on",
    "editor.defaultFormatter": "mervin.markdown-formatter"
  },
  "bookmarks.saveBookmarksInProject": true,
  "editor.formatOnSaveMode": "modifications",
  "maptz.regionfolder": {
    "[markdown]": {
      "defaultFoldStartRegex": "\\<!--[\\s]*#region[\\s]+/folded[\\s]*(.*)",
      "foldStart": "<!-- #region [NAME]",
      "foldStartRegex": "\\<!--[\\s]*#region[\\s]*(.*)",
      "foldEnd": "<!-- #endregion",
      "foldEndRegex": "\\<!--[\\s]*#endregion"
    },
    "[html]": {
      "defaultFoldStartRegex": "\\<!--[\\s]*#region[\\s]+/folded[\\s]*(.*)",
      "foldStart": "<!-- #region [NAME]",
      "foldStartRegex": "\\<!--[\\s]*#region[\\s]*(.*)",
      "foldEnd": "<!-- #endregion",
      "foldEndRegex": "\\<!--[\\s]*#endregion"
    },
    "[css]": {
      "defaultFoldStartRegex": "/\\*[\\s]*#region[\\s]+/folded(.*)",
      "foldStart": "/* #region [NAME]",
      "foldStartRegex": "/\\*[\\s]*#region[\\s]+(.*)",
      "foldEnd": "/* #endregion",
      "foldEndRegex": "/\\*[\\s]*#endregion(.*)"
    },
    "[scss]": {
      "defaultFoldStartRegex": "/\\*[\\s]*#region[\\s]+/folded(.*)",
      "foldStart": "/* #region [NAME]",
      "foldStartRegex": "/\\*[\\s]*#region[\\s]+(.*)",
      "foldEnd": "/* #endregion",
      "foldEndRegex": "/\\*[\\s]*#endregion(.*)"
    },
    "[javascript]": {
      "defaultFoldStartRegex": "//[\\s]*#region[\\s]+/folded[\\s]*(.*)",
      "foldStart": "// #region [NAME]",
      "foldStartRegex": "//[\\s]*#region[\\s]+(.*)",
      "foldEnd": "// #endregion",
      "foldEndRegex": "//[\\s]*#endregion(.*)"
    },
    "[typescript]": {
      "defaultFoldStartRegex": "//[\\s]*#region[\\s]+/folded[\\s]*(.*)",
      "foldStart": "// #region [NAME]",
      "foldStartRegex": "//[\\s]*#region[\\s]+(.*)",
      "foldEnd": "// #endregion",
      "foldEndRegex": "//[\\s]*#endregion(.*)"
    }
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "sort-imports.default-sort-style": "module-scoped",
  "sort-imports.on-save": true
}
